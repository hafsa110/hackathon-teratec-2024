/----------------------------------------------------------------------
/
/ This case demonstrate KHIONE's ability to model static border ice
/    growth in a meandering trapezoidal flume.
/
/ Steering file for the hydrodynamics: TELEMAC-2D
/
TITLE = 'BORDER ICE GROWTH IN A MEANDERING TRAPEZOIDAL FLUME'
/
/----------------------------------------------------------------------
/  FILES
/----------------------------------------------------------------------
/
GEOMETRY FILE                = 'geo_sineflume.slf'
BOUNDARY CONDITIONS FILE     = 'geo_sineflume.cli'
3D RESULT FILE               = 'r3d_border-ice.slf'
/
/----------------------------------------------------------------------
/  GENERAL INFORMATIONS - OUTPUTS
/----------------------------------------------------------------------
/
VARIABLES FOR 3D GRAPHIC PRINTOUTS  = 'Z,U,V'
/
GRAPHIC PRINTOUT PERIOD          = 180
LISTING PRINTOUT PERIOD          = 180
MASS-BALANCE                     = YES
TIME STEP                        = 1.
NUMBER OF TIME STEPS             = 360
NUMBER OF HORIZONTAL LEVELS     = 10
/
/----------------------------------------------------------------------
/  TRACER(S)
/----------------------------------------------------------------------
/
/COEFFICIENT FOR DIFFUSION OF TRACERS = 0.
/
/----------------------------------------------------------------------
/  INITIAL CONDITIONS
/----------------------------------------------------------------------
/
ORIGINAL DATE OF TIME      = 2016;12;2
ORIGINAL HOUR OF TIME      = 0;0;0
INITIAL TIME SET TO ZERO   = YES
/
COMPUTATION CONTINUED      = YES
PREVIOUS COMPUTATION FILE  = 'ini_border-ice.slf'
/
INITIAL VALUES OF TRACERS  = 0.002
/
/----------------------------------------------------------------------
/  BOUNDARY CONDITIONS
/----------------------------------------------------------------------
/
PRESCRIBED FLOWRATES          = -10.0  ; 10.0
PRESCRIBED ELEVATIONS         =   2.5  ;  2.5
VELOCITY PROFILES             =   4    ;  4
/
/ OPTION FOR LIQUID BOUNDARIES  =  2    ;  1
/
/ BOUND1 ( TEMPERATURE ); BOUND2 ...
PRESCRIBED TRACERS VALUES  =     0.0  ;  0.002
/
/----------------------------------------------------------------------
/  PHYSICAL PARAMETERS
/----------------------------------------------------------------------
/
TIDAL FLATS = YES
TREATMENT OF NEGATIVE DEPTHS = 2
OPTION FOR THE TREATMENT OF TIDAL FLATS = 1
/
LAW OF BOTTOM FRICTION  = 5
FRICTION COEFFICIENT FOR THE BOTTOM   = 0.5
LAW OF FRICTION ON LATERAL BOUNDARIES = 5
FRICTION COEFFICIENT FOR LATERAL SOLID BOUNDARIES = 0.5
NON-HYDROSTATIC VERSION = NO
/
/
/ VELOCITY DIFFUSIVITY  = 1.
/ OPTION FOR THE DIFFUSION OF VELOCITIES = 2
/ COEFFICIENT FOR DIFFUSION OF TRACERS   = 1.0
/ OPTION FOR THE DIFFUSION OF TRACERS = 2
/
/----------------------------------------------------------------------
/  NUMERICAL PARAMETERS
/----------------------------------------------------------------------
/
MASS-LUMPING FOR DEPTH = 1.
/
/----------------------------------------------------------------------
/  THERMIC PROCESS
/----------------------------------------------------------------------
/
COUPLING WITH          = 'KHIONE'
KHIONE STEERING FILE   = 'ice_border-ice.cas'
AIR PRESSURE           = YES
AIR TEMPERATURE = -15.
CLOUD COVER = 0.
/
/----------------------------------------------------------------------
/
