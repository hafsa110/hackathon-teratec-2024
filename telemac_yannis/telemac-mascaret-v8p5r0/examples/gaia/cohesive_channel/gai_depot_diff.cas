/-----------------------------------------------------------------------
/     PARAMETER FILES FOR GAIA
/-----------------------------------------------------------------------
BOUNDARY CONDITIONS FILE = geo_channel.cli
GEOMETRY FILE            = geo_channel.slf
RESULTS FILE             = gai_depot_diff.slf
/-----------------------------------------------------------------------
/ PARAMETERS FOR OUTPUTS
/-----------------------------------------------------------------------
VARIABLES FOR GRAPHIC PRINTOUTS = 'U,V,S,E,H,B,M,TOB,1A*,*ES'
MASS-BALANCE                    = YES
/-----------------------------------------------------------------------
/ NUMERICAL PARAMETERS
/-----------------------------------------------------------------------
TIDAL FLATS                             = YES
OPTION FOR THE TREATMENT OF TIDAL FLATS = 1
/---------------------------------------------------------------------
/ SUSPENSION
/---------------------------------------------------------------------
ADVECTION-DIFFUSION SCHEME WITH SETTLING VELOCITY = 1
SOLVER FOR DIFFUSION OF SUSPENSION   = 1
ACCURACY FOR DIFFUSION OF SUSPENSION = 1.D-6
MAXIMUM NUMBER OF ITERATIONS FOR SOLVER FOR SUSPENSION = 60
COEFFICIENT FOR DIFFUSION OF SUSPENDED SEDIMENTS = 75.
/-----------------------------------------------------------------------
/ TYPE OF SEDIMENT
/-----------------------------------------------------------------------
CLASSES TYPE OF SEDIMENT = CO
CLASSES SEDIMENT DENSITY = 2650.
CLASSES SEDIMENT DIAMETERS = 0.00001
CLASSES SETTLING VELOCITIES = 1.5D-4
/---------------------------------------------------------------------
/ TRANSPORT
/---------------------------------------------------------------------
BED LOAD FOR ALL SANDS                   = NO
BED-LOAD TRANSPORT FORMULA FOR ALL SANDS = 5
/
SUSPENSION FOR ALL SANDS                 = YES
SUSPENSION TRANSPORT FORMULA FOR ALL SANDS = 1
/---------------------------------------------------------------------/
/ NUMERICAL PARAMETERS
/---------------------------------------------------------------------
SCHEME FOR ADVECTION OF SUSPENDED SEDIMENTS = 1
/---------------------------------------------------------------------/
/ SURFACE BED PARAMETERS
/---------------------------------------------------------------------
SKIN FRICTION CORRECTION = 0
ACTIVE LAYER THICKNESS = 100.
FORMULA FOR SLOPE EFFECT                 = 0
FORMULA FOR DEVIATION                    = 0
SEDIMENT SLIDE                           = 0
/---------------------------------------------------------------------
/ BED MODEL without consolidation
/---------------------------------------------------------------------
BED MODEL = 1
/
NUMBER OF LAYERS FOR INITIAL STRATIFICATION = 1
LAYERS INITIAL THICKNESS = 100.D0
CLASSES INITIAL FRACTION = 1.D0
/
LAYERS MUD CONCENTRATION = 2650.
LAYERS CRITICAL EROSION SHEAR STRESS OF THE MUD = 10.
CLASSES CRITICAL SHEAR STRESS FOR MUD DEPOSITION = 0.1
LAYERS PARTHENIADES CONSTANT = 0.
